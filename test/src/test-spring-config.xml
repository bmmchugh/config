<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="testObject" class="com.frdna.config.context.TestObject">
    <property name="testField1" value="This is a test" />
    <property name="testField2" value="This is another test" />
  </bean>

  <bean id="overrideTestObject" class="com.frdna.config.context.TestObject">
    <property name="testField1" value="This is an override test" />
    <property name="testField2" value="This is another override test" />
  </bean>
</beans>


